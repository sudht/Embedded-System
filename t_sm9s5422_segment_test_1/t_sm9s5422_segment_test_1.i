 tcgetattr(0, &initial_settings);
 new_settings = initial_settings;
 new_settings.c_lflag &= ~0000002;
 new_settings.c_lflag &= ~0000010;
 new_settings.c_lflag &= ~0000001;
 new_settings.c_cc[5] = 1;

 new_settings.c_cc[5] = 0;
 tcsetattr(0, 0x5402, &new_settings);
}

void close_keyboard() {
 tcsetattr(0, 0x5402, &initial_settings);
}

int kbhit() {
 char ch;
 int nread;

 if (peek_character != -1) return 1;


 new_settings.c_cc[5] = 0;
 tcsetattr(0, 0x5402, &new_settings);
 nread = read(0, &ch, 1);
 new_settings.c_cc[5] = 1;
 tcsetattr(0, 0x5402, &new_settings);

 if (nread == 1) {
  peek_character = ch;
  return 1;
 }

 return 0;
}

int readch() {
 char ch;

 if (peek_character != -1) {

  ch = peek_character;
  peek_character = -1;
  return ch;
 }
 read(0, &ch, 1);
 return ch;
}


